#include "../include/cscommon.h"


extern u8 g_byLogLevel;

void logprintf(u8 byLevel,LPSTR lpstrFormat,...)
{
	if (byLevel < g_byLogLevel)
	{
		OspPrintf(TRUE,FALSE,lpstrFormat);
	}
}

